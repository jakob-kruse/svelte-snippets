{
  "Svelte: Component (TypeScript)": {
    "prefix": "svcomp",
    "body": ["<script lang=\"ts\">", "   $1", "</script>", "", "$0"],
    "description": "Svelte Component (TypeScript)"
  },
  "Svelte: Component (JavaScript)": {
    "prefix": "svcompjs",
    "body": ["<script>", "   $1", "</script>", "", "$0"],
    "description": "Svelte Component (JavaScript)"
  },
  "Svelte: Event Dispatcher (TypeScript)": {
    "prefix": "svdisp",
    "body": [
      "import { createEventDispatcher } from 'svelte';",
      "",
      "const dispatch = createEventDispatcher<{ $1: $2}>()",
      "$0"
    ],
    "description": "Svelte Event Dispatcher"
  },
  "Svelte: Event Dispatcher (JavaScript)": {
    "prefix": "svdispjs",
    "body": [
      "import { createEventDispatcher } from 'svelte';",
      "",
      "const dispatch = createEventDispatcher()",
      "$0"
    ],
    "description": "Svelte Event Dispatcher (JavaScript)"
  },
  "Svelte: Head Tag": {
    "prefix": "svhead",
    "body": ["<svelte:head>", "   $1", "</svelte:head>", "$0"],
    "description": "Svelte Head Tag"
  },
  "Svelte: For Each": {
    "prefix": "svfe",
    "body": ["{#each $1 as $2 ($3)}", "   $0", "{/each}", "$0"],
    "description": "Svelte For Each"
  },
  "Svelte: For Each else": {
    "prefix": "svfee",
    "body": [
      "{#each $1 as $2 ($3)}",
      "   $4",
      "{:else}",
      "   $5",
      "{/each}",
      "$0"
    ],
    "description": "Svelte For Each else"
  },
  "Svelte: If": {
    "prefix": "svif",
    "body": ["{#if $1}", "   $2", "{/if}", "$0"],
    "description": "Svelte If"
  },
  "Svelte: If Else": {
    "prefix": "svife",
    "body": ["{#if $1}", "   $2", "{:else}", "   $3", "{/if}", "$0"],
    "description": "Svelte If Else"
  },
  "Svelte: If Else If": {
    "prefix": "svifei",
    "body": ["{#if $1}", "   $2", "{:else if $3}", "   $4", "{/if}", "$0"],
    "description": "Svelte If Else If"
  },
  "Svelte: Await": {
    "prefix": "svawait",
    "body": ["{#await $1}", "   $2", "{:then $3}", "   $4", "{/await}", "$0"],
    "description": "Svelte Await"
  },
  "Svelte: Key": {
    "prefix": "svkey",
    "body": ["{#key $1}", "   $2", "{/key}", "$0"],
    "description": "Svelte Key"
  }
}
